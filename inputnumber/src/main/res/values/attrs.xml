<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputNumberView">
        <!--最小值-->
        <attr name="numberMin" format="float" />
        <attr name="isFloat" format="boolean" />
        <!--最大值-->
        <attr name="numberMax" format="float" />
        <!--默认值-->
        <attr name="defaultNumber" format="integer" />
        <!--禁用-->
        <attr name="btnDisable" format="boolean" />
        <!--step 增加减少的间隔数-->
        <attr name="step" format="float" />
    </declare-styleable>
</resources>